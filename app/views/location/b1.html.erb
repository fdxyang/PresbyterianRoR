<div class="container-fluid main-container">
  <%= render 'location/leftMenu' %>
  <div class="col-md-10 content">
    <article class="panel panel-default">
      <header class="panel-heading">

      </header>
      <section class="panel-body">
        <img src="/images/floor/b1/plan.jpg" class="content-img full">
        <!-- todo carousel module start-->
        <div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="5000">
              <!-- Indicators -->
              <ol class="carousel-indicators">
                <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                <li data-target="#myCarousel" data-slide-to="1" class=""></li>
                <li data-target="#myCarousel" data-slide-to="2" class=""></li>
                <li data-target="#myCarousel" data-slide-to="3" class=""></li>
              </ol>
              <div class="carousel-inner" role="listbox">
                <div class="item active">
                  <img src="/images/floor/b1/1.jpg" >
                </div>
                <div class="item">
                  <img src="/images/floor/b1/2.jpg" >
                </div>
                <div class="item">
                  <img src="/images/floor/b1/3.jpg" >
                </div>
                <div class="item">
                  <img src="/images/floor/b1/4.jpg" >
                </div>
              </div>
              <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
                <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
              </a>
              <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
                <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
              </a>
        </div>
        <!-- todo carousel module end-->
        <i class="fa fa-circle">  副堂</i><br>
        <i class="fa fa-circle">  愛餐區</i><br>
        <i class="fa fa-circle">  兒童遊戲區</i><br>
        <i class="fa fa-circle">  卡拉ＯＫ區</i><br>
        <i class="fa fa-circle">  角落咖啡區</i>
      </section>
    </article>
  </div>
</div>


