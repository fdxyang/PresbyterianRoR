new Witness
    <%= form_for @witness, :url => { :controller => 'witness', :action => 'create' } do |f| %>
    <div class="form-group">
        <%= f.label :title, "title" ,:for=>"witness_title" %>
        <%= f.text_field :title ,:class=>"form-control" ,:id=>"witness_title" %>
    </div>
    <div class="form-group">
      <%= f.label :photo, "photo" ,:for=>"witness_photo" %>
      <%= f.text_field :photo ,:class=>"form-control" ,:id=>"witness_photo" %>
    </div>
    <div class="form-group">
        <%= f.label :content, "content", :for=>"witness_content" %>
        <%= f.text_area :content ,:class=>"form-control" ,:id=>"witness_content" %>
    </div>

    <%= f.submit "Create", :class=>"btn btn-default" %>
<% end %>